# Enter your code here. Read input from STDIN. Print output to STDOUT
Num = int(input())

for i in range(0, Num):

    string = input()

    for j in range(0, len(string)):
        if j % 2 == 0:
            print(string[j], end='')

    print(" ", end='')

    for j in range(0, len(string)):
        if j % 2 != 0:
            print(string[j], end='')

    print("")
